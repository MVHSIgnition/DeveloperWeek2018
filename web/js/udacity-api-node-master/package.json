{
  "name": "udacity-api",
  "version": "0.3.2",
  "description": "unofficial udacity client library for node.js",
  "main": "index.js",
  "scripts": {
    "pretest": "npm install && ./node_modules/.bin/jshint src/ test/",
    "test": "mocha test/specs/*.js",
    "clean": "rm -rf node_modules && npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tylucaskelley/udacity-api"
  },
  "keywords": [
    "api",
    "udacity",
    "library",
    "json",
    "education",
    "courses",
    "school",
    "data"
  ],
  "author": "Ty-Lucas Kelley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tylucaskelley/udacity-api/issues"
  },
  "homepage": "http://www.tylucaskelley.com",
  "dependencies": {
    "bx": "^0.3.0",
    "request": "^2.51.0"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "jshint": "^2.5.11",
    "mocha": "^2.0.1"
  },
  "jshintConfig": {
    "node": true,
    "unused": true,
    "undef": true,
    "expr": true,
    "predef": [
      "describe",
      "it"
    ]
  }
}
